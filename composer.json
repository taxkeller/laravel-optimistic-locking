    "require-dev": {
        "ext-pdo_sqlite": "*",
        "mockery/mockery": "^1.0.0",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit" : "^10.0"
    },
    "autoload": {
        "psr-4": {
